<load>

<config>
    <server value="http://beaker-stage.app.eng.bne.redhat.com" />
    <xmlproxy value="/RPC2" />
</config>

<sessions>
<session duration='24' unit='hour'>
           <request id='distros-list-xmlrpc' interval='1' unit='seconds' type='xmlrpc' method='distros.filter'>
               <param name='limit' value=''/>
                <param name='name' value=''/>
                <param name='treepath' value=''/>
                <param name='family' value=''/>
                <param name='arch' value=''/>
                <param name='tags' value=''/>
           </request>
   </session>

<session duration='24' unit='hour'>
           <request id='distros-list' interval='10' unit='seconds' type='client' cmd='bkr distros-list'/>
   </session>
</sessions>

</load>
