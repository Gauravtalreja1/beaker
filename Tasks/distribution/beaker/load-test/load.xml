<root>
<config>
    <xmlproxy value="/RPC2" />
        <baseload session='create_jobs' requests='1' unit='minute'/>
            <baseload session='job_watch' requests='83'/> <!-- This equates to roughly 10k an hour -->
            </config>

            <load>
                <user duration='10' unit='minute' session='create_jobs' load-level='1'/>
                    <user duration='1' unit='minute' session='job_watch' load-level='0.1' run-once='1'/>

                        <user delay='5' duration='5' unit='minute' session='job_watch' load-level='0.1' run-once='1'/>

                            <user delay='10' duration='5' unit='minute' session='job_watch' load-level='0.1' run-once='1'/>
                            </load>

                            </root>

